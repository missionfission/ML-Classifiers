    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.cluster import KMeans\n",
    "data = pd.read_csv('Medical_data.csv')\n",
    "X = data.as_matrix()\n",
    "Y = X[:,1:4]\n",
    "kmeans = KMeans(n_clusters = 3)\n",
    "kmeans.fit(Y)\n",
    "print(kmeans.cluster_centers_)"
